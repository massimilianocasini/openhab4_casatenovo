rule "zwave:device:77c679d29e:node2"
when Thing "zwave:device:77c679d29e:node2" changed
// to “OFFLINE (COMMUNICATION_ERROR)“
then
val String thingUID = "zwave:device:77c679d29e:node2"
var thingStatusInfo = getThingStatusInfo(thingUID)
// && (thingStatusInfo.getStatus().toString() == "ONLINE")) {
    if (thingStatusInfo !== "ONLINE" && Prodino0_optoIn2.state == "220VOK")  {
        val telegramAction = getActions("telegram","telegram:telegramBot:Telegram_Bot")
        telegramAction.sendTelegram(thingUID + " " + thingStatusInfo.getStatus().toString())
        logError("", "Thing status: " + thingUID + " " + thingStatusInfo.getStatus().toString() )
    } return
end

